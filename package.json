{
  "name": "setup-maven",
  "version": "1.0.0",
  "private": true,
  "description": "setup maven action",
  "main": "dist/index.js",
  "scripts": {
    "build": "ncc build src/setup-maven.ts",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "prerelease": "npm run-script build",
    "release": "git add -f dist/index.js",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jvalkeal/setup-maven.git"
  },
  "keywords": [
    "actions",
    "node",
    "setup"
  ],
  "author": "jvalkeal",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.2.6",
    "@actions/exec": "^1.0.1",
    "@actions/io": "^1.0.2",
    "@actions/tool-cache": "^1.6.0",
    "semver": "^6.1.1",
    "typed-rest-client": "1.5.0"
  },
  "devDependencies": {
    "@types/jest": "^24.0.13",
    "@types/node": "^16.15.0",
    "@types/semver": "^6.0.0",
    "@vercel/ncc": "^0.38.1",
    "jest": "^24.8.0",
    "jest-circus": "^24.7.1",
    "prettier": "^1.19.1",
    "ts-jest": "^24.0.2",
    "typescript": "^5.2.2"
  },
  "husky": {
    "skipCI": true,
    "hooks": {
      "pre-commit": "npm run build && npm run format"
    }
  }
}
